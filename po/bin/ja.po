msgstr "guide: Gentoo Linux の xml ドキュメントフォーマット。"

#. ."\n  - ".gettext("html: HTML documents (EXPERIMENTAL).")
#: ../../lib/Locale/Po4a/Chooser.pm:65
msgid "ini: .INI format."
msgstr "ini: .INI フォーマット。"

#: ../../lib/Locale/Po4a/Chooser.pm:66
