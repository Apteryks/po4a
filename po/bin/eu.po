msgstr "2 plural formatu baino gehiagoko mezuak ez dira onartzen."

#: ../../lib/Locale/Po4a/Po.pm:304
#, perl-format
msgid "Strange line: -->%s<--"
msgstr "Lerro arraroa: -->%s<--"

